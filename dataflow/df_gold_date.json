{
	"name": "df_gold_date",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_products",
						"type": "DatasetReference"
					},
					"name": "Products"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_silver_sales",
						"type": "DatasetReference"
					},
					"name": "Sales"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_silver_warranty",
						"type": "DatasetReference"
					},
					"name": "Warranty"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "join1"
				},
				{
					"name": "select2"
				},
				{
					"name": "cast1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PRODUCT_ID as string,",
				"          PRODUCT_NAME as string,",
				"          CATEGORY_ID as string,",
				"          LAUNCH_DATE as date,",
				"          PRICE as decimal(12,2),",
				"          NOTES as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Products",
				"source(output(",
				"          SALE_ID as string,",
				"          PRODUCT_ID as string,",
				"          STORE_ID as string,",
				"          SALE_DATE as date,",
				"          QUANTITY as integer,",
				"          REVENUE as decimal(14,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Sales",
				"source(output(",
				"          CLAIM_ID as string,",
				"          CLAIM_DATE as string,",
				"          SALE_ID as string,",
				"          REPAIR_STATUS as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Warranty",
				"Products select(mapColumn(",
				"          PRODUCT_ID,",
				"          PRODUCT_NAME,",
				"          CATEGORY_ID,",
				"          SALE_DATE = LAUNCH_DATE,",
				"          PRICE,",
				"          NOTES",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1, cast1 join(select1@SALE_DATE == cast1@SALE_DATE,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"Warranty select(mapColumn(",
				"          CLAIM_ID,",
				"          SALE_DATE = CLAIM_DATE,",
				"          SALE_ID,",
				"          REPAIR_STATUS",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 cast(output(",
				"          SALE_DATE as date 'yyyy-MM-dd'",
				"     ),",
				"     errors: true) ~> cast1"
			]
		}
	}
}