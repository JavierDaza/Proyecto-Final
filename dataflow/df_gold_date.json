{
	"name": "df_gold_date",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_products",
						"type": "DatasetReference"
					},
					"name": "Products"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_silver_sales",
						"type": "DatasetReference"
					},
					"name": "Sales"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_silver_warranty",
						"type": "DatasetReference"
					},
					"name": "Warranty"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "select2"
				},
				{
					"name": "select3"
				},
				{
					"name": "union1"
				},
				{
					"name": "cast2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PRODUCT_ID as string,",
				"          PRODUCT_NAME as string,",
				"          CATEGORY_ID as string,",
				"          LAUNCH_DATE as date,",
				"          PRICE as decimal(12,2),",
				"          NOTES as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Products",
				"source(output(",
				"          SALE_ID as string,",
				"          PRODUCT_ID as string,",
				"          STORE_ID as string,",
				"          SALE_DATE as date,",
				"          QUANTITY as integer,",
				"          REVENUE as decimal(14,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Sales",
				"source(output(",
				"          CLAIM_ID as string,",
				"          CLAIM_DATE as string,",
				"          SALE_ID as string,",
				"          REPAIR_STATUS as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Warranty",
				"Products select(mapColumn(",
				"          SALE_DATE = LAUNCH_DATE",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"Sales select(mapColumn(",
				"          SALE_DATE",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"cast2 select(mapColumn(",
				"          SALE_DATE = CLAIM_DATE",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"select1, select2, select3 union(byName: true)~> union1",
				"Warranty cast(output(",
				"          CLAIM_DATE as date 'yyyy-MM-dd'",
				"     ),",
				"     errors: true) ~> cast2"
			]
		}
	}
}