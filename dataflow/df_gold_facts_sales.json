{
	"name": "df_gold_facts_sales",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_gold_sales",
						"type": "DatasetReference"
					},
					"name": "Sales"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_date",
						"type": "DatasetReference"
					},
					"name": "Date"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_product",
						"type": "DatasetReference"
					},
					"name": "Product"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_store",
						"type": "DatasetReference"
					},
					"name": "Store"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_warranty",
						"type": "DatasetReference"
					},
					"name": "Warranty"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_gold_facts_sales",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "surrogateKey2"
				},
				{
					"name": "join1"
				},
				{
					"name": "surrogateKey3"
				},
				{
					"name": "surrogateKey4"
				},
				{
					"name": "surrogateKey5"
				},
				{
					"name": "surrogateKey6"
				},
				{
					"name": "join2"
				},
				{
					"name": "surrogateKey7"
				},
				{
					"name": "join3"
				},
				{
					"name": "surrogateKey8"
				},
				{
					"name": "join4"
				},
				{
					"name": "select1"
				},
				{
					"name": "MapDrifted1",
					"description": "Crea una asignación explícita para cada columna de desfase."
				},
				{
					"name": "select2"
				},
				{
					"name": "MapDrifted2",
					"description": "Crea una asignación explícita para cada columna de desfase."
				},
				{
					"name": "MapDrifted3",
					"description": "Crea una asignación explícita para cada columna de desfase."
				},
				{
					"name": "MapDrifted4",
					"description": "Crea una asignación explícita para cada columna de desfase."
				},
				{
					"name": "MapDrifted5",
					"description": "Crea una asignación explícita para cada columna de desfase."
				}
			],
			"scriptLines": [
				"source(output(",
				"          SALE_ID as string,",
				"          STORE_ID as string,",
				"          SALE_DATE as date,",
				"          PRODUCT_ID as string,",
				"          QUANTITY as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Sales",
				"source(output(",
				"          date_id as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Date",
				"source(output(",
				"          PRODUCT_ID as string,",
				"          PRODUCT_NAME as string,",
				"          CATEGORY_NAME as string,",
				"          PRICE as decimal(12,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Product",
				"source(output(",
				"          STORE_NAME as string,",
				"          CITY as string,",
				"          STORE_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Store",
				"source(output(",
				"          warranty_id as string,",
				"          warranty_type as string,",
				"          SALE_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Warranty",
				"MapDrifted1 keyGenerate(output(ID_1 as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"Date keyGenerate(output(ID_2 as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey2",
				"surrogateKey1, surrogateKey2 join(ID_1 == ID_2,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"select2 keyGenerate(output(ID_3 as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey3",
				"Store keyGenerate(output(ID_4 as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey4",
				"Warranty keyGenerate(output(ID_5 as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey5",
				"MapDrifted2 keyGenerate(output(ID_A as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey6",
				"surrogateKey6, surrogateKey3 join(ID_A == ID_3,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"MapDrifted3 keyGenerate(output(ID_B as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey7",
				"surrogateKey7, surrogateKey4 join(ID_B == ID_4,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join3",
				"MapDrifted4 keyGenerate(output(ID_C as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey8",
				"surrogateKey8, surrogateKey5 join(ID_C == ID_5,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join4",
				"MapDrifted5 select(mapColumn(",
				"          SALE_ID = Sales@SALE_ID,",
				"          QUANTITY,",
				"          date_id,",
				"          PRODUCT_NAME,",
				"          CATEGORY_NAME,",
				"          PRICE,",
				"          STORE_NAME,",
				"          CITY,",
				"          warranty_id,",
				"          warranty_type,",
				"          STORE_ID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"Sales derive(STORE_ID = toString(byName('STORE_ID'))) ~> MapDrifted1",
				"Product select(mapColumn(",
				"          PRODUCT_NAME,",
				"          CATEGORY_NAME,",
				"          PRICE",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"join1 derive(STORE_ID = toString(byName('STORE_ID'))) ~> MapDrifted2",
				"join2 derive(STORE_ID = toString(byName('STORE_ID'))) ~> MapDrifted3",
				"join3 derive(STORE_ID = toString(byName('STORE_ID'))) ~> MapDrifted4",
				"join4 derive(STORE_ID = toString(byName('STORE_ID'))) ~> MapDrifted5",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          SALE_ID as string,",
				"          PRODUCT_ID as string,",
				"          PRODUCT_NAME as string,",
				"          CATEGORY_NAME as string,",
				"          PRICE as decimal(12,2),",
				"          STORE_NAME as string,",
				"          CITY as string,",
				"          DATE_ID as date,",
				"          QUANTITY as integer,",
				"          WARRANTY_ID as string,",
				"          WARRANTY_TYPE as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          SALE_ID,",
				"          PRODUCT_NAME,",
				"          CATEGORY_NAME,",
				"          PRICE,",
				"          STORE_NAME,",
				"          CITY,",
				"          DATE_ID = date_id,",
				"          QUANTITY,",
				"          WARRANTY_ID = warranty_id,",
				"          WARRANTY_TYPE = warranty_type",
				"     )) ~> sink1"
			]
		}
	}
}