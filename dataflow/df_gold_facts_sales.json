{
	"name": "df_gold_facts_sales",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_gold_date",
						"type": "DatasetReference"
					},
					"name": "Date"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_product",
						"type": "DatasetReference"
					},
					"name": "Product"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_store",
						"type": "DatasetReference"
					},
					"name": "Store"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_warranty",
						"type": "DatasetReference"
					},
					"name": "Warranty"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_gold_facts_sales",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "surrogateKey2"
				},
				{
					"name": "surrogateKey3"
				},
				{
					"name": "surrogateKey4"
				},
				{
					"name": "join1"
				},
				{
					"name": "surrogateKey5"
				},
				{
					"name": "join2"
				},
				{
					"name": "surrogateKey6"
				},
				{
					"name": "join3"
				}
			],
			"scriptLines": [
				"source(output(",
				"          date_id as date,",
				"          year as integer,",
				"          month as integer,",
				"          day as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Date",
				"source(output(",
				"          PRODUCT_ID as string,",
				"          PRODUCT_NAME as string,",
				"          CATEGORY_NAME as string,",
				"          PRICE as decimal(12,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Product",
				"source(output(",
				"          STORE_ID as string,",
				"          STORE_NAME as string,",
				"          CITY as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Store",
				"source(output(",
				"          warranty_id as string,",
				"          warranty_type as string,",
				"          duration_months as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Warranty",
				"Date keyGenerate(output(ID_A as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"Product keyGenerate(output(ID_B as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey2",
				"Store keyGenerate(output(ID_C as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey3",
				"Warranty keyGenerate(output(ID_D as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey4",
				"surrogateKey1, surrogateKey2 join(ID_A == ID_B,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 keyGenerate(output(ID_E as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey5",
				"surrogateKey5, surrogateKey3 join(ID_E == ID_C,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join2 keyGenerate(output(ID_F as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey6",
				"surrogateKey6, surrogateKey4 join(ID_F == ID_D,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join3",
				"join3 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          ID_REGISTRO as integer,",
				"          SALE_ID as string,",
				"          PRODUCT_ID as string,",
				"          DATE_ID as date,",
				"          QUANTITY as integer,",
				"          TOTAL_AMOUNT as decimal(12,2),",
				"          WARRANTY_ID as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}