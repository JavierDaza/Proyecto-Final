{
	"name": "df_gold_facts_sales",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_gold_date",
						"type": "DatasetReference"
					},
					"name": "Date"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_product",
						"type": "DatasetReference"
					},
					"name": "Product"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_store",
						"type": "DatasetReference"
					},
					"name": "Stores"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_gold_facts_sales",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "surrogateKey2"
				},
				{
					"name": "join1"
				},
				{
					"name": "surrogateKey3"
				},
				{
					"name": "join2"
				},
				{
					"name": "surrogateKey4"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          date_id as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Date",
				"source(output(",
				"          product_id as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Product",
				"source(output(",
				"          store_id as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Stores",
				"Product keyGenerate(output(ID_A as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"Stores keyGenerate(output(ID_B as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey2",
				"surrogateKey1, surrogateKey2 join(ID_A == ID_B,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"Date keyGenerate(output(ID_C as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey3",
				"surrogateKey3, surrogateKey4 join(ID_C == ID_D,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join1 keyGenerate(output(ID_D as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey4",
				"join2 select(mapColumn(",
				"          date_id,",
				"          product_id,",
				"          store_id",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          product_id as string,",
				"          store_id as string,",
				"          date_id as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}