{
	"name": "df_silver_warranty",
	"properties": {
		"folder": {
			"name": "Silver"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_warranty",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_warranty",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "cast1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          claim_id as string,",
				"          claim_date as string,",
				"          sale_id as string,",
				"          repair_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"cast1 derive(sale_id = iif(length(trim(sale_id)) == 0, 'N/A', trim(sale_id)),",
				"          claim_date = iif(\r",
				"    !isNull(toDate(claim_date, 'MM/dd/yyyy')),\r",
				"    toString(toDate(claim_date, 'MM/dd/yyyy'), 'yyyy-MM-dd'),\r",
				"    iif(\r",
				"        !isNull(toDate(claim_date, 'dd-MM-yyyy')),\r",
				"        toString(toDate(claim_date, 'dd-MM-yyyy'), 'yyyy-MM-dd'),\r",
				"        toString(toDate(claim_date, 'yyyy/MM/dd'), 'yyyy-MM-dd')\r",
				"    )\r",
				")) ~> derivedColumn1",
				"source1 cast(output(",
				"          claim_id as string",
				"     ),",
				"     errors: true) ~> cast1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          CLAIM_ID as string,",
				"          CLAIM_DATE as string,",
				"          SALE_ID as string,",
				"          REPAIR_STATUS as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          CLAIM_ID = claim_id,",
				"          CLAIM_DATE = claim_date,",
				"          SALE_ID = sale_id,",
				"          REPAIR_STATUS = repair_status",
				"     )) ~> sink1"
			]
		}
	}
}