{
	"name": "df_silver_category",
	"properties": {
		"folder": {
			"name": "Silver"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_category",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_category",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn3"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CATEGORY_ID as string,",
				"          CATEGORY_NAME as string,",
				"          EXTRA_NOTE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"derivedColumn2 derive(EXTRA_NOTE = iif(isNull(EXTRA_NOTE) || length(trim(EXTRA_NOTE)) == 0, 'N/A', EXTRA_NOTE),",
				"          CATEGORY_ID = replace(replace(CATEGORY_ID, 'cat', 'CAT'), '', 'CAT'),",
				"          CATEGORY_NAME = iif(length(trim(CATEGORY_NAME)) == 0, 'N/A', trim(CATEGORY_NAME))) ~> derivedColumn1",
				"source1 derive(CATEGORY_ID = iif(isNull(CATEGORY_ID), split(CATEGORY_NAME, ' ')[1], CATEGORY_ID),",
				"          CATEGORY_NAME = iif(isNull(CATEGORY_ID), 'Missing Name', CATEGORY_NAME),",
				"          EXTRA_NOTE = iif(isNull(CATEGORY_ID), split(CATEGORY_NAME, ' ')[2], EXTRA_NOTE)) ~> derivedColumn2",
				"derivedColumn1 select(mapColumn(",
				"          CATEGORY_ID,",
				"          CATEGORY_NAME",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 derive(CATEGORY_NAME = trim(lower(CATEGORY_NAME))) ~> derivedColumn3",
				"derivedColumn3 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          CATEGORY_SK as integer,",
				"          CATEGORY_ID as string,",
				"          CATEGORY_NAME as string,",
				"          EXTRA_NOTE as string,",
				"          IS_DUPLICATE as boolean",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}